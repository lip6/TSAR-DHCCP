gal Processeur {
	int state = $PRO_READY ;
	int addr = 0 ;

	//int myid = $id;
	//    
	//    transition transitionName [myid < 3] {
	//    	myid +=1;
	//    }

	transition write (addr_t $addr) [true] label "write" ($addr) {
		addr = $addr ;
	}
	transition read (addr_t $addr) [addr == $addr] label "read" ($addr) {
		addr = 0 ;
	}
	transition t_Ready_WaitRD [state == $PRO_READY] label "Ready_WaitRD" {
		state = $PRO_WAIT_RD ;
	}
